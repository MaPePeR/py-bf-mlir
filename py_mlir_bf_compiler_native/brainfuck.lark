// tokens: 
MOVE_RIGHT: ">"
MOVE_LEFT: "<"
INCREMENT: "+"
DECREMENT: "-"
OUTPUT: "."
INPUT: ","
LOOP_START: "["
LOOP_END: "]"

start: _instruction*

_instruction: ( loop | _op )
loop: LOOP_START (_instruction*) LOOP_END
_op: MOVE_RIGHT
    | MOVE_LEFT
    | INCREMENT
    | DECREMENT
    | OUTPUT
    | INPUT

COMMENT: /[^<>+.,\[\]-]/+

%ignore COMMENT